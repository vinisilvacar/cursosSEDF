//Arquivo para abrir e fechar conteudo da página Usando BDQ no SAE.

function startMenu(div){
    div.style.display = "none";
}
                
function abreFechaTopico(div){
     if(div.style.display == "none"){
           div.style.display = "block";
     }else{
           startMenu(div);
     } 
}

